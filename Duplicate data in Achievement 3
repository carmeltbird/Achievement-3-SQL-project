###Duplicate Data

SELECT DISTINCT film_id,
title,
description,
release_year,
language_id,
rental_duration,
rental_rate,
Length,
replacement_cost,
rating,
last_update,
special_ features,
fulltextï¼Œ
COUNT (*)
FROM film
GROUP BY film_id,
title,
description,
release_year,
language_id,
rental_rate,
length,
replacement_cost,
rating,
last_update,
special_features,
fulltext
HAVING COUNT (*) > 1;

